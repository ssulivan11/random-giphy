{
  "manifest_version": 2,
  "name": "Random Giphy",
  "description": "This extension adds a gif to your clipboard formatted as a markdown according specified tags",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "author": "Demi [demipel8@gmail.com]",
   "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+A",
        "mac": "Command+Shift+A",
        "chromeos": "Ctrl+Shift+A",
        "linux": "Ctrl+Shift+A"
      },
      "description": "Get Giphy"
    }
  },
  "minimum_chrome_version": "45.0",
  "offline_enabled": false,
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "clipboardWrite",
    "https://tbdsa3t4af.execute-api.us-east-2.amazonaws.com/v1/*",
    "storage"
  ],
  "short_name": "RG"
}
