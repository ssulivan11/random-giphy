{
  "name": "random-giphy",
  "description": "Chrome extention for a random giphy return in markup format for things like reddit, PRs, and MRs. Simply send it a list of random things to get random gifs.",
  "version": "2.0.0",
  "license": "ISC",
  "author": "ssulivan11",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssulivan11/random-giphy"
  },
  "keywords": [
    "giphy"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "webpack src/js/optionsPage.js --o src/options.js --f iife -p",
    "build:zip": "rm -f randomGiphy.zip && zip -r randomGiphy.zip src",
    "lint": "eslint '**/*.js'",
    "prettier": "prettier --write '**/*.{js,json,md}'",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn prettier",
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.3",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "resolutions": {
    "set-value": "2.0.1",
    "mixin-deep": "1.3.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 97,
        "branches": 100,
        "functions": 94,
        "lines": 100
      }
    }
  }
}